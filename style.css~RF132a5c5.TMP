*, :before, :after{
    -webkit-box-sizing:border-box;
    -moz-box-sizing:border-box;
    box-sizing:border-box
}

a,a:focus,a:hover{
    color:#fff;
}

@media(min-width:768px){
    .header{
        float:right;
    }
    .mastfoot-inner{
        float:right
    }
}

@media(min-width:768px){
    .header{
        position:fixed;
        top:0
    }
    .mastfoot-inner{
        position:fixed;
        bottom:0
    }
    .header,.mastfoot-inner,.container{
        width:100%
    }
}

@media(min-width:992px){
    .header,.mastfoot-inner,.container{
        width:700px}
}


/* body and body* set background color and font size*/
body {
  font-size: 150%;
  line-height: 1.8em;
  background: #000;
  color: #fff;
  margin: 30px;
}

 
/* below gives nav menu white border above */
* header#header nav ul li {
  border-top: 1px solid #aeaeae;
  list-style-type: none;
  display: block;
}

/* below hides navigation sub-menues */
header#header nav #primary li ul {
  display: none;
}
/* below gives lines above nav menu color during hover*/
header#header nav #primary li:hover,
header#header nav #primary li.active {
  border-color: #00ff00;
  color: #00ff00;
}

/* below gives sub-menues display */
header#header nav #primary li:hover ul,
header#header nav #primary li.active ul {
  display: block;
}

/* below gives menu color when hovering */
header#header nav #primary li:hover > a,
header#header nav #primary li.active > a {
  color: #00ff00;
}

/* controls menu bar sizing and layout */
*.row > [class*="span"] {
  display: inline;
  float: left;
  margin-left: 10px;
}

.span2 {
  width: 150px;
}

div.mastfoot {
  position: absolute;
  bottom: 0;
  border-top: 1px solid white;
  left: 10px;
  color: #aeaeae;
  width: 100%;
}


